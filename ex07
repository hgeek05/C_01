#include <unistd.h>

void    ft_putchar(char c)
{
    write(1, &c, 1);
}

void	ft_rev_int_tab(int *tab, int size)
{
	int	rev[size];
	int	i;
	int	SIZE;

	i = 0;
	SIZE = size;
	while (size > 0)
	{
		rev[i] = tab[size - 1];
		i++;
		size--;
	}
	i = 0;
	while (i < SIZE)
	{
		tab[i] = rev[i];
		i++;
	}
}



int main(int i)
{
    int arr[5] = {1,2,3,4,5};

    ft_rev_int_tab(arr, 5);

    for(i = 0; i < 5; i++)
    {
        ft_putchar(arr[i] + '0');
    }

    return 0;
}


